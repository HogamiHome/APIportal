   "components":{
      "schemas":{
         "SingapurResponse":{
            "type":"object",
            "properties":{
               "status":{
                  "type":"string"
               },
               "code":{
                  "type":"string"
               },
               "message":{
                  "type":"string"
               },
               "data":{
                  "type":"object"
               }
            }
         },
         "SingapurResponseListLong":{
            "type":"object",
            "properties":{
               "status":{
                  "type":"string"
               },
               "code":{
                  "type":"string"
               },
               "message":{
                  "type":"string"
               },
               "data":{
                  "type":"array",
                  "items":{
                     "type":"integer",
                     "format":"int64"
                  }
               }
            }
         },
         "Pageable":{
            "type":"object",
            "properties":{
               "page":{
                  "minimum":0,
                  "type":"integer",
                  "format":"int32"
               },
               "size":{
                  "minimum":1,
                  "type":"integer",
                  "format":"int32"
               },
               "sort":{
                  "type":"array",
                  "items":{
                     "type":"string"
                  }
               }
            }
         },
         "PageRatingRest":{
            "type":"object",
            "properties":{
               "totalPages":{
                  "type":"integer",
                  "format":"int32"
               },
               "totalElements":{
                  "type":"integer",
                  "format":"int64"
               },
               "size":{
                  "type":"integer",
                  "format":"int32"
               },
               "content":{
                  "type":"array",
                  "items":{
                     "$ref":"#/components/schemas/RatingRest"
                  }
               },
               "number":{
                  "type":"integer",
                  "format":"int32"
               },
               "first":{
                  "type":"boolean"
               },
               "sort":{
                  "$ref":"#/components/schemas/Sort"
               },
               "numberOfElements":{
                  "type":"integer",
                  "format":"int32"
               },
               "last":{
                  "type":"boolean"
               },
               "pageable":{
                  "$ref":"#/components/schemas/PageableObject"
               }
            }
         },
         "PageableObject":{
            "type":"object",
            "properties":{
               "offset":{
                  "type":"integer",
                  "format":"int64"
               },
               "sort":{
                  "$ref":"#/components/schemas/Sort"
               },
               "pageSize":{
                  "type":"integer",
                  "format":"int32"
               },
               "pageNumber":{
                  "type":"integer",
                  "format":"int32"
               },
               "unpaged":{
                  "type":"boolean"
               },
               "paged":{
                  "type":"boolean"
               }
            }
         },
         "RatingRest":{
            "required":[
               "generalRating"
            ],
            "type":"object",
            "properties":{
               "id":{
                  "type":"integer",
                  "description":"Id of the rating",
                  "format":"int64"
               },
               "createDate":{
                  "type":"integer",
                  "description":"Rating creation date",
                  "format":"int64"
               },
               "clientName":{
                  "type":"string",
                  "description":"Rating client name"
               },
               "professionalName":{
                  "type":"string",
                  "description":"Rating professional name"
               },
               "generalRating":{
                  "type":"number",
                  "description":"General rating of the professional",
                  "format":"double"
               },
               "opinion":{
                  "type":"string",
                  "description":"Opinion about the professional"
               },
               "punctual":{
                  "type":"number",
                  "description":"Rating the punctuality of the professional",
                  "format":"double"
               },
               "behavior":{
                  "type":"number",
                  "description":"Rating the behavior of the professional",
                  "format":"double"
               },
               "satisfied":{
                  "type":"number",
                  "description":"Satisfaction rating about the professional",
                  "format":"double"
               },
               "clean":{
                  "type":"number",
                  "description":"Rating of the cleanliness of the professional",
                  "format":"double"
               },
               "operationId":{
                  "type":"integer",
                  "format":"int64"
               },
               "titleOfferedService":{
                  "type":"string"
               },
               "createDateShow":{
                  "type":"string"
               },
               "updateDateShow":{
                  "type":"string"
               }
            },
            "description":"This model represents the Ratings"
         },
         "SingapurResponsePageRatingRest":{
            "type":"object",
            "properties":{
               "status":{
                  "type":"string"
               },
               "code":{
                  "type":"string"
               },
               "message":{
