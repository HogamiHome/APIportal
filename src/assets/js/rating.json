               },
               "unsorted":{
                  "type":"boolean"
               }
            }
         },
         "AverageRatingRest":{
            "type":"object",
            "properties":{
               "professionalId":{
                  "type":"integer",
                  "format":"int64"
               },
               "averageGeneralRating":{
                  "type":"number",
                  "format":"double"
               },
               "averagePunctual":{
                  "type":"number",
                  "format":"double"
               },
               "averageBehavior":{
                  "type":"number",
                  "format":"double"
               },
               "averageSatisfied":{
                  "type":"number",
                  "format":"double"
               },
               "averageClean":{
                  "type":"number",
                  "format":"double"
               },
               "totalAverage":{
                  "type":"integer",
                  "format":"int32"
               }
            },
            "description":"This model represents the average for all ratings for a professional"
